<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:layout_width="409dp"
    android:layout_height="729dp"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="1dp"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <Button
        android:id="@+id/allContact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Show all contact" />

    <Button
        android:id="@+id/callLog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Access the call log" />

    <Button
        android:id="@+id/mediaStore"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Access media store" />

    <Button
        android:id="@+id/bookMarks"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Access Bookmarks" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"></LinearLayout>

</LinearLayout>